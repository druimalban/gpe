/*
 * Generate discard code for a scope change.
 */
JavaGenDiscard(ILNode_ScopeChange)
{
	if(node->body)
	{
		ILScope *oldScope = info->currentScope;
		info->currentScope = node->scope;
		JavaGenDiscard(node->body, info);
		info->currentScope = oldScope;
	}
}

