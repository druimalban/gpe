#: main.c:85
msgid "Valid options:\n"
msgstr "Opsi valid:\n"

#: main.c:86
msgid "   -g GEOMETRY  window geometry (default: 240x100+0+220)\n"
msgstr "  -g GEOMETRY  jendela geometri (default: 240x100+0+220)\n"

