#: main.c:85
msgid "Valid options:\n"
msgstr "Geçerli seçenekler:\n"

#: main.c:86
msgid "   -g GEOMETRY  window geometry (default: 240x100+0+220)\n"
msgstr "   -g GEOMETRY pencere geometrisi (öntanýmlý: 240x100+0+220)\n"

#: main.c:87
msgid "   -k           always keep window on top (override redirect)\n"
msgstr "   -k           herzaman pencereyi üstte tut (yönlendirmeyi baskýn tut)\n"

#: main.c:88
msgid "   -h           this help text\n"
msgstr "   -h           bu yardým metnidir\n"

