cmd3 = Contacts;gpe-contacts
cmd4 = To-do List;gpe-todo
cmd5 = Taskmanager;gpe-taskmanager
cmd6 = Audio Player;gpe-nmf
cmd7 = Web Browser;gpe-mini-browser
cmd8 = Mail;sylpheed
cmd9 = Audio Mixer;gpe-mixer
