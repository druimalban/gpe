	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(apmSCRIPT_INSTALL) '$$d$$p' '$(DESTDIR)$(apmdir)/$$f'"; \
	    $(apmSCRIPT_INSTALL) "$$d$$p" "$(DESTDIR)$(apmdir)/$$f"; \
	  else :; fi; \
	done

