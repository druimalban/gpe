install-data-hook:
	cd $(DESTDIR)$(apmdir) && mv gpe-login.suspend S98lock-display
	cd $(DESTDIR)$(xsessiondir) && mv gpe-login.session 50autolock
	cd $(DESTDIR)$(xinitdir) && mv gpe-login.xinit 99gpe-login
	cd $(DESTDIR)$(sysdir) && mv gpe-login.sysconfig gpelogin

uninstall-hook:
	$(RM) -f $(DESTDIR)$(apmdir)/S98lock-display
	$(RM) -f $(DESTDIR)$(xsessiondir)/50autolock
	$(RM) -f $(DESTDIR)$(xinitdir)/99gpe-login
	$(RM) -f $(DESTDIR)$(sysdir)/gpelogin

clean-local:
	rm -f *.d *~

deb: dist
	-mkdir $(top_builddir)/debian-build
	cd $(top_builddir)/debian-build && tar xzf $(top_builddir)/../$(PACKAGE)-$(VERSION).tar.gz
	cp -r $(top_builddir)/debian $(top_builddir)/debian-build/$(PACKAGE)-$(VERSION)
	cd $(top_builddir)/debian-build/$(PACKAGE)-$(VERSION) && dpkg-buildpackage -rfakeroot
	-rm -rf $(top_builddir)/debian-build/$(PACKAGE)-$(VERSION)

dist-upload: dist
	scp $(PACKAGE)-$(VERSION).tar.bz2 $(USER)@linuxtogo.org:/media/data/projects/gpe/source/

tag:
	cvs tag $(CVSTAG)

retag:
	cvs tag -F $(CVSTAG)

source: tag dist-upload

#-- Fetch .po files from TP
freshen-po: linguas
	rm -rf tmp-po
	mkdir tmp-po
#	cd tmp-po; for LANG in $(LINGUAS); do wget $(TRANSLATION_SITE)/$(PACKAGE)/$$LANG.po; done
	cd tmp-po; ../$(DIR_TP)/po-wget-last ../$(DIR_TP)/postats $(PACKAGE)
	for LANG in $$(cat linguas); do if [ ! -f $(DIR_PO)/$$LANG.po ] || ! cmp -s $(DIR_PO)/$$LANG.po tmp-po/$$LANG.po ; then mv tmp-po/$$LANG.po $(DIR_PO)/$$LANG.po; echo "Updated $$LANG translation"; fi; done
	rm -rf tmp-po

#-- Extract LINGUAS from TP stat file
linguas: $(DIR_TP)/postats
	$(DIR_TP)/po-extract-linguas $(DIR_TP)/postats $(PACKAGE) > linguas
	@echo "linguas:::: " $(LINGUAS)

#-- Fetch TP stat file
$(DIR_TP)/postats: freshen-postats
freshen-postats:
	@echo "++++++++++++++++++++++++++++++++++++++"
	@echo "Freshen "$(DIR_TP)"/postats:"
	cd $(DIR_TP) && wget --timestamping http://www.iro.umontreal.ca/translation/data/postats
	@echo "++++++++++++++++++++++++++++++++++++++"
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
