#: gpe-login.c:524 gpe-login.c:604
msgid "Login incorrect"
msgstr "ログイン失敗"

#: gpe-login.c:657 gpe-login.c:739
msgid "Passwords don't match"
msgstr "パスワードが合っていません"

#: gpe-login.c:666 gpe-login.c:748
msgid "Empty password not allowed"
msgstr "空のパスワードは使えません"

#: gpe-login.c:728
msgid "User already exists"
msgstr "ユーザーは既に存在します"

#: gpe-login.c:1336
msgid "Holding down the stylus for greater than"
msgstr "スタイラスペンを"

#: gpe-login.c:1337
msgid "5 seconds"
msgstr "5 秒"

#: gpe-login.c:1338
msgid ", or holding"
msgstr "より長く押し続けるか、"

#: gpe-login.c:1339
msgid "record"
msgstr "記録"

